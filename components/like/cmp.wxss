/* components/like/cmp.wxss */


.container {
    position: relative;
    width: 50rpx;
    height: 50rpx;
}

.like, .unlike {
    position: absolute;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    border: 4rpx solid #000;
    border-radius: 50%;
}

.like {
    background-color: #000;
}